        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }	 
	var start = g_form.getValue("start_time");
	var duration = g_form.getValue("planned_duration");	
	
	var ga = new GlideAjax('x_813980_charge_0.ChargerClientScripts');   //Scriptinclude
	ga.addParam('sysparm_name', 'addDurationToStart'); //Method
	ga.addParam('sysparm_start',start); //Parameters
	ga.addParam('sysparm_duration',duration); //Parameters
	ga.getXML(getResponse);
	
	function getResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");
		g_form.setValue('planned_end_time',answer);
	}	  
}]]></script>
